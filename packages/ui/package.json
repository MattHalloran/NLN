        "build": "react-scripts build --max-old-space-size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096",
        "start-development": "react-scripts start",
        "start-production": "serve -s build -l 3000",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze": "source-map-explorer build/static/js/*",
        "graphql-generate": "apollo client:codegen --target typescript --endpoint=http://localhost:5329/api/v1 --outputFlat --includes \"src/graphql/**/*.ts\" --excludes \"src/graphql/generated\" src/graphql/generated"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],

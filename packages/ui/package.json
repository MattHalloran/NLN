{
    "name": "ui",
    "version": "2.2.5",
    "main": "index.ts",
    "type": "module",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.14.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@hello-pangea/dnd": "^18.0.1",
        "@local/shared": "*",
        "@mui/icons-material": "^7.3.2",
        "@mui/lab": "^5.0.0-alpha.177",
        "@mui/material": "^5.18.0",
        "apollo-upload-client": "^17.0.0",
        "formik": "^2.2.8",
        "graphql": "^15.5.0",
        "graphql-tag": "^2.12.4",
        "graphql-ws": "^5.16.0",
        "immutability-helper": "^3.1.1",
        "jspdf": "^3.0.2",
        "jspdf-autotable": "^5.0.2",
        "lodash-es": "^4.17.21",
        "lucide-react": "^0.544.0",
        "markdown-to-jsx": "^7.1.7",
        "react": "^18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.3.0",
        "react-gallery-carousel": "^0.1.4",
        "react-lazily": "^0.9.0",
        "typescript": "^5.9.2",
        "xmlbuilder": "^15.1.1",
        "zxcvbn": "^4.4.2"
    },
    "scripts": {
        "build": "tsc && vite build && node workbox-build.js",
        "build:analyze": "tsc && vite build --config analyze-bundle.js",
        "start-development": "vite",
        "start-production": "serve --config serve.json",
        "analyze": "npm run build:analyze && open dist/bundle-stats.html",
        "analyze:legacy": "source-map-explorer --html \"source-tree-$(date '+%Y%m%d%H%M%S').html\" --no-border-checks dist/assets/*",
        "graphql-generate": "apollo client:codegen --target typescript --endpoint=http://localhost:5331/api/v1 --outputFlat --includes \"src/api/**/*.ts\" --excludes \"src/api/generated\" src/api/generated",
        "lint": "eslint src",
        "typecheck": "tsc --noEmit"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "devDependencies": {
        "@types/apollo-upload-client": "^17.0.0",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^19.1.15",
        "@types/react-dom": "^19.1.9",
        "@types/react-gallery-carousel": "^0.2.2",
        "@types/zxcvbn": "^4.4.5",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "eslint": "^8.35.0",
        "eslint-config-react-app": "^7.0.1",
        "rollup-plugin-visualizer": "^5.12.0",
        "source-map-explorer": "^2.5.3",
        "vite": "^5.4.11",
        "vite-plugin-compression": "^0.5.1",
        "workbox-build": "^7.3.0"
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    }
}
